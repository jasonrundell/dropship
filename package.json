  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "bugs": {
    "url": "https://github.com/jasonrundell/dropship/issues"
  },
  "homepage": "https://github.com/jasonrundell/dropship#readme",
